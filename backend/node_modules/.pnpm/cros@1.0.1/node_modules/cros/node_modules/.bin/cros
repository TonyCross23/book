#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/side/node/book-shop/backend/node_modules/.pnpm/cros@1.0.1/node_modules/cros/bin/node_modules:/home/tony/side/node/book-shop/backend/node_modules/.pnpm/cros@1.0.1/node_modules/cros/node_modules:/home/tony/side/node/book-shop/backend/node_modules/.pnpm/cros@1.0.1/node_modules:/home/tony/side/node/book-shop/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tony/side/node/book-shop/backend/node_modules/.pnpm/cros@1.0.1/node_modules/cros/bin/node_modules:/home/tony/side/node/book-shop/backend/node_modules/.pnpm/cros@1.0.1/node_modules/cros/node_modules:/home/tony/side/node/book-shop/backend/node_modules/.pnpm/cros@1.0.1/node_modules:/home/tony/side/node/book-shop/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
